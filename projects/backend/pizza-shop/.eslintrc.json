{
  "extends": [
    "@rocketseat/eslint-config/node",
    "plugin:drizzle/all"
  ],
  "plugins": ["simple-import-sort", "drizzle"],
  "root": true,
  "rules": {
    "no-case-declarations": "off",
    "@typescript-eslint/no-namespace": "off",
    "camelcase": "off",
    "@typescript-eslint/no-empty-function": "off",
    "simple-import-sort/imports": "error"
  }
}
